<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout

    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg6"
    tools:context=".Lecture_Final">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">


        <Button
            android:id="@+id/btnFinalBack"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="28dp"
            android:layout_marginLeft="28dp"
            android:layout_marginTop="41dp"
            android:layout_marginEnd="34dp"
            android:layout_marginRight="34dp"
            android:background="@drawable/backbutton"
            app:layout_constraintEnd_toStartOf="@+id/textView9"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="189dp"
            android:layout_height="45dp"
            android:layout_gravity="center_horizontal"
            android:layout_marginStart="112dp"

            android:layout_marginLeft="112dp"
            android:layout_marginTop="44dp"
            android:layout_marginEnd="112dp"
            android:layout_marginRight="112dp"
            android:text="Final Period"
            android:textColor="#000"
            android:textSize="30dp"
            android:textStyle="bold"
            android:typeface="serif"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Spinner
            android:id="@+id/spinnerfinalone"
            android:layout_width="99dp"
            android:layout_height="49dp"
            android:layout_marginEnd="68dp"
            android:layout_marginRight="68dp"
            android:background="@drawable/roundb_forspinner"
            android:spinnerMode="dropdown"
            app:layout_constraintBottom_toBottomOf="@+id/finalone"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/finalone"
            app:layout_constraintVertical_bias="0.0" />

        <Button
            android:id="@+id/finalone"
            android:layout_width="205dp"
            android:layout_height="49dp"
            android:layout_gravity="center_horizontal"
            android:layout_marginStart="67dp"
            android:layout_marginLeft="67dp"
            android:layout_marginTop="88dp"
            android:layout_marginEnd="100dp"
            android:layout_marginRight="100dp"
            android:background="@drawable/roundb"
            android:text="Basic Input / Output"
            android:textColor="#FFFFFF"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.694"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView9" />

        <Spinner
            android:id="@+id/spinnerfinaltwo"
            android:layout_width="103dp"
            android:layout_height="49dp"
            android:background="@drawable/roundb_forspinner"
            android:spinnerMode="dropdown"
            app:layout_constraintBottom_toBottomOf="@+id/finaltwo"
            app:layout_constraintEnd_toEndOf="@+id/spinnerfinalone"
            app:layout_constraintTop_toTopOf="@+id/finaltwo"
            app:layout_constraintVertical_bias="0.0" />

        <Button
            android:id="@+id/finaltwo"
            android:layout_width="203dp"
            android:layout_height="49dp"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="48dp"
            android:background="@drawable/roundb"
            android:text="Swing"
            android:textColor="#FFFFFF"
            app:layout_constraintEnd_toEndOf="@+id/finalone"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/finalone"
            app:layout_constraintTop_toBottomOf="@+id/finalone" />

        <Spinner
            android:id="@+id/spinnerfinalthree"
            android:layout_width="107dp"
            android:layout_height="47dp"
            android:background="@drawable/roundb_forspinner"
            android:spinnerMode="dropdown"
            app:layout_constraintBottom_toBottomOf="@+id/finalthree"
            app:layout_constraintEnd_toEndOf="@+id/spinnerfinaltwo"
            app:layout_constraintTop_toTopOf="@+id/finalthree"
            app:layout_constraintVertical_bias="1.0" />

        <Button
            android:id="@+id/finalthree"
            android:layout_width="204dp"
            android:layout_height="47dp"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="48dp"
            android:background="@drawable/roundb"
            android:text="Applets"
            android:textColor="#FFFFFF"
            app:layout_constraintEnd_toEndOf="@+id/finaltwo"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/finaltwo"
            app:layout_constraintTop_toBottomOf="@+id/finaltwo" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</RelativeLayout>